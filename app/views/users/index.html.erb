<section class="users-index">

  <header class="page-header">
    <h1>Admin &raquo; All Users</h1>
  </header>

  <div class="well">
    <%= link_to '+ New User', [:new, :user], class: 'btn btn-info' %>
  </div>

  <div class="panel panel-default products">

    <table class="table table-bordered">
      <thead>
        <tr>
          <th>first_name</th>
          <th>last_name</th>
          <th>email</th>
          <th>password</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <%= render @users %>
      </tbody>

    </table>

  </div>

</section>
